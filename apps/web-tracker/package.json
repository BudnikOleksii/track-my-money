{
  "name": "web-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit --incremental false",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint ./ --ext .ts,.tsx --fix",
    "stylelint": "stylelint '**/*.{scss,css}'",
    "stylelint:fix": "stylelint '**/*.{scss,css}' --fix",
    "generate:api-types": "swagger-typescript-api -p ../../api/openapi.json -o ./src/api/generated -n Api.ts --http-client-type fetch --generate-client --generate-route-types --generate-responses --extract-request-params --extract-request-body --extract-enums --enum-names-as-values --generate-union-enums --single-http-client --clean-output"
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.2",
    "@reduxjs/toolkit": "2.10.1",
    "@track-my-money/ui": "workspace:*",
    "async-mutex": "0.5.0",
    "deepmerge": "4.3.1",
    "next": "16.0.1",
    "next-intl": "4.5.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "7.66.0",
    "react-redux": "9.2.0",
    "redux-persist": "6.0.0",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@track-my-money/eslint-config": "workspace:*",
    "swagger-typescript-api": "13.2.16",
    "@track-my-money/stylelint-config": "workspace:*",
    "@track-my-money/typescript-config": "workspace:*",
    "@types/node": "24.10.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "9.39.1",
    "eslint-config-next": "16.0.1",
    "redux-persist-transform-encrypt": "5.1.1",
    "stylelint": "16.25.0",
    "typescript": "5.9.3"
  }
}
